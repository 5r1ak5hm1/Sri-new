#include <stdio.h>
int main() {
    int team1[4], team2[4];
    printf("Enter scores of Team 1 in 4 games:\n");
    for (int i = 0; i < 4; i++) scanf("%d", &team1[i]);
    printf("Enter scores of Team 2 in 4 games:\n");
    for (int i = 0; i < 4; i++) scanf("%d", &team2[i]);
    int total1 = 0, total2 = 0;
    for (int i = 0; i < 4; i++) {
        total1 += team1[i];
        total2 += team2[i];
    }
    printf("Total score Team 1: %d\n", total1);
    printf("Total score Team 2: %d\n", total2);
    return 0;
}
