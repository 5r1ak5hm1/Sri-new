#include <stdio.h>
int main() {
    int n;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    int sum = 0;
    for (int i = 0; i < n; i++) {
        int num;
        printf("Enter number %d: ", i + 1);
        scanf("%d", &num);
        sum += num;
    }
    printf("Average: %.2f\n", (float)sum / n);
    return 0;
}
